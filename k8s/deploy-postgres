apiVersion: apps/v1
kind: Deployment
metadata:
  name: demo-postgres-d
  labels:
    app: demo-spring-boot
    tier: db
spec:
  selector:
    matchLabels:
      app: demo-postgres
  replicas: 1
  template:
    metadata:
      labels:
        app: demo-postgres
    spec:
      containers:
      - name: demo-postgres
        image: dperezcabrera/demo-spring-boot:0.0.1
        ports:
        - containerPort: 8080
